{
  "manifest_version": 2,
  "name": "Go Code Review Comments",
  "description": "Web extension providing context menus to insert links for Go code review topics",
  "version": "1.0.0",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "assets/go_16.png",
    "48": "assets/go_48.png",
    "128": "assets/go_128.png"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ]
}